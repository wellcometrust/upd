{% render '@site-header' %}

{% if header.layout %}

    {% render '@page-header--' + header.layout %}

{% else %}

    {% render '@page-header', header %}

{% endif %}

<div class="wrapper  wrapper--gutter">

    {% if layout %}

        {% render '@layout--' + layout %}

    {% else %}

        {% include '@layout' %}

    {% endif %}

</div>
