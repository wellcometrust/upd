<div class="resource-links">
  <div class="wrapper wrapper--gutter">
    <h3 class="h3 resource-links__title">{{ title }}</h2>
    <ul class="list-bare">

      {% for item in items %}
        <li>
          <a class="external-link" href="{{item.link}}">{{ item.title }}</a> ({{item.source}})
        </li>
      {% endfor %}
    </ul>
  </div>
</div>
