<a class="case-study-tile" href="#link-to-destination">

    <img alt="" class="case-study-tile__image" src={{path image}}>

    <div class="case-study-tile__info">

        <h{{level}} class="case-study-tile__title">{{title}}</h{{level}}>

        <div class="case-study-tile__tag">

            {{> @tag}}

        </div>

        {{#cta}}

            {{> @call-to-action}}

        {{/cta}}

    </div>

</a>