<div class="team-tile">

    <img alt="" class="team-tile__image" src="{{ image | path }}">

    <span class="team-tile__name">{{ name }}</span>

    <span class="team-tile__title">{{ title }}</span>

    <a class="team-tile__contact" href="#link-to-twitter-account">

        {% render '@svg-icon', { circular: true, icon: 'twitter', title: 'Twitter'} %}

    </a>

</div>
