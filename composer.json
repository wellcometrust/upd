{
  "name": "ce/drupal",
  "description": "",
  "type": "project",
  "license": "GPL-2",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": {
    "drupal-org": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "dropzone": {
      "type": "package",
      "package": {
        "name": "dropzone/dropzone.js",
        "version": "4.3.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "dropzone"
        },
        "dist": {
          "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "colorbox": {
      "type": "package",
      "package": {
        "name": "colorbox/colorbox.js",
        "version": "1.6.4",
        "type": "drupal-library",
        "extra": {
          "installer-name": "colorbox"
        },
        "dist": {
          "url": "https://github.com/jackmoore/colorbox/archive/master.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "ce_deploy": {
      "type": "package",
      "package": {
        "name": "ce/ce_deploy",
        "type": "drupal-custom-module",
        "version": "1.0",
        "source": {
          "type": "git",
          "url": "git@git.codeenigma.com:ce-drupal-dev-megazord/ce_deploy.git",
          "reference": "8.x-1.x"
        }
      }
    }
  },
  "require": {
    "composer/installers": "1.*",
    "dropzone/dropzone.js": "4.3.0",
    "colorbox/colorbox.js": "1.6.4",
    "drupal-composer/drupal-scaffold": "2.*",
    "drupal/config_ignore": "2.x",
    "drupal/core": "8.5.*",
    "drupal/ctools": "3.*",
    "drupal/facets": "1.*",
    "drupal/jquery_colorpicker": "1.*",
    "drupal/layout_plugin": "1.*",
    "drupal/metatag": "^1.2",
    "drupal/page_manager": "4.x",
    "drupal/panels": "4.*",
    "drupal/paragraphs": "1.*",
    "drupal/pathauto": "1.*",
    "drupal/search_api": "1.*",
    "drupal/search_api_solr": "^1.0",
    "drupal/simple_sitemap": "^2.9",
    "drupal/video_embed_field": "2.x",
    "drupal/google_analytics": "2.x",
    "drupal/entity_reference_display": "1.x",
    "drupal/config_filter": "1.x",
    "drupal/mailchimp": "1.x-dev",
    "drupal/admin_toolbar": "1.x",
    "drupal/console": "~1.0",
    "drupal/media_entity_browser": "2.x",
    "drupal/media_entity_video": "2.x",
    "drupal/media_upload": "2.x",
    "drupal/dropzonejs": "2.x",
    "drupal/Inline_entity_form": "1.x",
    "drupal/image_widget_crop": "2.x",
    "drupal/colorbox": "1.x",
    "drupal/colorbox_load": "1.x",
    "drupal/menu_link_attributes": "1.x",
    "drupal/field_group": "3.0-beta1"
  },
  "require-dev": {
    "drupal/devel": "1.*"
  },
  "replace": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "patches-file": "composer.patches.json",
    "enable-patching": true,
    "installer-paths": {
      "www/core": ["type:drupal-core"],
      "www/modules/contrib/{$name}": ["type:drupal-module"],
      "www/modules/custom/{$name}": ["type:drupal-custom-module"],
      "www/profiles/contrib/{$name}": ["type:drupal-profile"],
      "www/themes/contrib/{$name}": ["type:drupal-theme"],
      "www/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "www/contrib/{$name}": ["type:drupal-drush"],
      "www/libraries/{$name}": ["type:drupal-library"]
    }
  },
  "config": {
    "process-timeout": 2200
  }
}
