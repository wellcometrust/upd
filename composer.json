{
  "name": "ce/drupal",
  "description": "Content website for the 'Understanding Patient Data' project.",
  "type": "project",
  "license": "GPL-2",
  "homepage": "https://understandingpatientdata.org.uk/",
  "authors": [
    {
      "name": "Code Enigma",
      "role": "Maintainer"
    }
  ],
  "repositories": {
    "drupal-org": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "dropzone": {
      "type": "package",
      "package": {
        "name": "dropzone/dropzone.js",
        "version": "4.3.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "dropzone"
        },
        "dist": {
          "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "colorbox": {
      "type": "package",
      "package": {
        "name": "colorbox/colorbox.js",
        "version": "1.6.4",
        "type": "drupal-library",
        "extra": {
          "installer-name": "colorbox"
        },
        "dist": {
          "url": "https://github.com/jackmoore/colorbox/archive/master.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    },
    "ce_deploy": {
      "type": "package",
      "package": {
        "name": "ce/ce_deploy",
        "type": "drupal-custom-module",
        "version": "1.0",
        "source": {
          "type": "git",
          "url": "git@git.codeenigma.com:ce-drupal-dev-megazord/ce_deploy.git",
          "reference": "8.x-1.x"
        }
      }
    },
    "ckeditor_tabletoolstoolbar": {
      "type": "package",
      "package": {
        "name": "ckeditor/tabletoolstoolbar",
        "version": "0.0.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/tabletoolstoolbar/releases/tabletoolstoolbar_0.0.1.zip",
          "type": "zip"
        }
      }
    }
  },
  "require": {
    "ckeditor/tabletoolstoolbar": "^0.0.1",
    "colorbox/colorbox.js": "1.6.4",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "dropzone/dropzone.js": "4.3.0",
    "drupal/admin_toolbar": "^3.0",
    "drupal/ckeditor_tabletoolstoolbar": "^2.0",
    "drupal/color_field": "^2.4",
    "drupal/colorbox": "^1.0",
    "drupal/colorbox_load": "^1.0",
    "drupal/config_filter": "^1.0",
    "drupal/config_split": "^1.4",
    "drupal/console": "~1.0",
    "drupal/core": "^9.2",
    "drupal/core-composer-scaffold": "^9.2",
    "drupal/ctools": "^3.0",
    "drupal/devel": "^4.0",
    "drupal/draggableviews": "^2.0",
    "drupal/dropzonejs": "2.x",
    "drupal/entity_reference_display": "^1.0",
    "drupal/eu_cookie_compliance": "^1.9",
    "drupal/facets": "^2.0",
    "drupal/field_group": "^3.0",
    "drupal/google_analytics": "^2.0",
    "drupal/image_widget_crop": "^2.1",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/mailchimp": "^2.0",
    "drupal/media_entity_browser": "^2.0",
    "drupal/media_entity_video": "^2.0",
    "drupal/menu_link_attributes": "^1.0",
    "drupal/metatag": "^1.2",
    "drupal/module_filter": "^3.1",
    "drupal/page_manager": "^4.0@beta",
    "drupal/panels": "^4.0",
    "drupal/paragraphs": "^1.0",
    "drupal/pathauto": "^1.0",
    "drupal/pdfpreview": "^1.0@RC",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/redirect": "^1.5",
    "drupal/scheduled_publish": "^3.1",
    "drupal/search_api": "^1.16",
    "drupal/search_api_solr": "^4.1",
    "drupal/simple_sitemap": "^4.0",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/video_embed_field": "^2.0",
    "drush/drush": "^9.7.1 | ^10.0.0",
    "joachim-n/composer-manifest": "^1.1",
    "webflo/drupal-finder": "^1.0.0",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "require-dev": {
    "geerlingguy/drupal-vm": "4.*"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "process-timeout": 2200,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "zaporylie/composer-drupal-optimizations": true,
      "drupal/console-extend-plugin": true,
      "joachim-n/composer-manifest": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "www/"
      }
    },
    "patches-file": "composer.patches.json",
    "enable-patching": true,
    "installer-paths": {
      "www/core": ["type:drupal-core"],
      "www/libraries/{$name}": ["type:drupal-library"],
      "www/modules/contrib/{$name}": ["type:drupal-module"],
      "www/profiles/contrib/{$name}": ["type:drupal-profile"],
      "www/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "www/modules/custom/{$name}": ["type:drupal-custom-module"],
      "www/themes/custom/{$name}": ["type:drupal-custom-theme"]
    }
  }
}
